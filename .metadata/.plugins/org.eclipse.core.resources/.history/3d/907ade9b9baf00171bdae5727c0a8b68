package telephoneWords;

import java.util.Scanner;

public class TelNoWords {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String no = sc.next();
		getNameFromNumber(no ,"");
		sc.close();
	}
	

	public static void getNameFromNumber(String str, String name){
		if (str.length()==1){
			
			/*for(int i = 0 ; i <str.length() ; i++){
				
			}*/
			System.out.println(name);
		}
		else{
			String passedStr = str.substring(1);
			for (int j = 0 ; j < str.length(); j++){
				/*for(int k = 0 ; k < 3; k++){
					
				}*/
				
				getNameFromNumber(passedStr ,name + str.charAt(j);
				
			}
		}
	}
	
	public static char getCharKey(int no , int loc){
		char[][] array = {{'0'},{'1'},{'A','B','C'},{'D','E','F'},{'G','H','I'},{'J','K','L'},
							{'M','N','O'},{'P','R','S'},{'T','U','V'},{'W','X','Y'}};
		
		return array[no][loc];
	}
}
